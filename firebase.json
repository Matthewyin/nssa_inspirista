{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "id": "nssa_inspirista"
    }
  },
  "apphosting": [
    {
      "backends": {
        "nssa_inspirista": {
          "region": "us-central1",
          "source": ".",
          "framework": "nextjs"
        }
      }
    },
    {
      "backendId": "nssa-inspirista",
      "rootDir": "/",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "functions"
      ]
    }
  ]
}
